is_pressed():\n                pass\n            EEPROM.writew(1, timeHoldEA)\n            onSetup = 2  \n    elif onSetup == 2:\n        if input.button_is_pressed(Button.A):\n            while input.button_is_pressed(Button.A):\n                pass\n            timeDelayEA = timeDelayEA + 2\n        if input.button_is_pressed(Button.B):\n            while input.button_is_pressed(Button.B):\n                pass\n            timeDelayEA = timeDelayEA - 2\n            if timeDelayEA < 0:\n                timeDelayEA = 0\n        if input.logo_is_pressed():\n            while input.logo_is_pressed():\n                pass\n            EEPROM.writew(0, timeDelayEA)\n            onSetup = 0 \n    basic.pause(100)\nbasic.forever(on_forever)\n\ndef on_forever2():\n    global motor\n    if motor == 1:\n        pins.analog_write_pin(AnalogPin.P13, 0)\n        pins.analog_write_pin(AnalogPin.P14, 400)\n        basic.pause(timeHoldEA*1000)\n        motor = 0\n    else:\n        pins.analog_write_pin(AnalogPin.P13, 0)\n        pins.analog_write_pin(AnalogPin.P14, 0)\nbasic.forever(on_forever2)\n\ndef on_forever3():\n    global timeEA, motor, onSetup\n    if timeDelay >= 10:\n        I2C_LCD1602.show_number(timeDelay, 8, 0)\n    else:\n        I2C_LCD1602.show_number(0, 8, 0)\n        I2C_LCD1602.show_number(timeDelay, 9, 0)\n    if time >= 10:\n        I2C_LCD1602.show_number(time, 11, 1)\n    else:\n        I2C_LCD1602.show_number(0, 11, 1)\n        I2C_LCD1602.show_number(time, 12, 1)\n    basic.pause(1000)\n    time = time - 1\n    if time <= 0:\n        time = timeDelay\n        motor = 1\nbasic.forever(on_forever3)\n\ndef on_handle_icon():\n    global motor\n    if motor == 1:\n        basic.show_icon(IconNames.HEART, 500)\n        basic.show_icon(IconNames.SMALL_HEART, 500)\n    else:\n        basic.show_icon(IconNames.SMALL_HEART, 100)\nbasic.forever(on_handle_icon)","pxt.json":"{\n    \"name\": \"ebbAndFlow_v1\",\n    \"description\": \"\",\n    \"dependencies\": {\n