 . . . #\n            . # # # .\n            \"\"\",\n        0)\n    basic.pause(de)\nmotor = 0\nde = 0\npins.analog_write_pin(AnalogPin.P13, 0)\npins.analog_write_pin(AnalogPin.P14, 0)\nI2C_LCD1602.lcd_init(39)\nI2C_LCD1602.on()\nI2C_LCD1602.backlight_on()\nI2C_LCD1602.show_string(\"INIT SYSTEM\", 2, 0)\nbasic.show_leds(\"\"\"\n        . # # # .\n        # . . . #\n        # . . . #\n        # . . . #\n        . # # # .\n        \"\"\",\n    100)\nfor index in range(5):\n    loading()\nI2C_LCD1602.show_string(\"INIT COMPLETE\", 1, 0)\nbasic.show_icon(IconNames.HEART)\nbasic.pause(1000)\ntimeDelay = 10\ntime = timeDelay\n# default timmer\nI2C_LCD1602.clear()\nI2C_LCD1602.show_string(\"TIMER_SET: \", 0, 0)\nI2C_LCD1602.show_string(\"TIME_C: \", 0, 1)\n\ndef on_forever():\n    global timeDelay\n    if input.button_is_pressed(Button.A):\n        while input.button_is_pressed(Button.A):\n            pass\n        timeDelay = timeDelay + 2\n    if input.button_is_pressed(Button.B):\n        while input.button_is_pressed(Button.B):\n            pass\n        timeDelay = timeDelay - 2\n    basic.pause(100)\nbasic.forever(on_forever)\n\ndef on_forever2():\n    global motor\n    if motor == 1:\n        motor = 0\n        basic.show_icon(IconNames.HAPPY)\n        pins.analog_write_pin(AnalogPin.P13, 0)\n        pins.analog_write_pin(AnalogPin.P14, 400)\n        basic.pause(3000)\n    else:\n        basic.show_icon(IconNames.ASLEEP)\n        pins.analog_write_pin(AnalogPin.P13, 0)\n        pins.analog_write_pin(AnalogPin.P14, 0)\nbasic.forever(on_forever2)\n\ndef on_forever3():\n    global time, motor\n    if timeDelay >= 10:\n        I2C_LCD1602.show_number(timeDelay, 12, 0)\n    else:\n        I2C_LCD1602.show_number(0, 12, 0)\n        I2C_LCD1602.show_number(timeDelay, 13, 0)\n    if time >= 10:\n        I2C_LCD1602.show_number(time, 11, 1)\n    else:\n        I2C_LCD1602.show_number(0, 11, 1)\n        I2C_LCD1602.show_number(time, 12, 1)\n    basic.pause(1000)\n    time = time - 1\n    if time <= 0:\n        time = timeDelay\n        motor = 1\nbasic.forever(on_forever3)\n","pxt.json":"{\n    \"name\": \"ebbAndFlow_v1\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"lightning-and-temperature-sensing-system\": \"github:STEMax-Tech/lightning-and-temperature-sensing-system#v0.0.2\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}}],"shares":[],"lastSaveTime":1729786326532}