old - 5 \n            if valueThreshold < 0:\n                valueThreshold = 0 \n        if input.logo_is_pressed():\n            while input.logo_is_pressed():\n                pass\n            EEPROM.writew(2, valueThreshold)\n            onPart = 0  \n    elif onSetup == 1: #On ESA\n            if input.button_is_pressed(Button.A):\n                while input.button_is_pressed(Button.A):\n                    pass\n                timeHold = timeHold + 2\n            if input.button_is_pressed(Button.B):\n                while input.button_is_pressed(Button.B):\n                    pass\n                timeHold = timeHold - 2  \n                if timeHold < 0:\n                    timeHold = 0\n            if input.logo_is_pressed():\n                while input.logo_is_pressed():\n                    pass\n                EEPROM.writew(1, timeHold)\n                onSetup = 2  \n        else:\n            if input.button_is_pressed(Button.A):\n                while input.button_is_pressed(Button.A):\n                    pass\n                timeDelay = timeDelay + 2\n            if input.button_is_pressed(Button.B):\n                while input.button_is_pressed(Button.B):\n                    pass\n                timeDelay = timeDelay - 2\n            if input.logo_is_pressed():\n                while input.logo_is_pressed():\n                    pass\n                EEPROM.writew(0, timeDelay)\n                onSetup = 1 \n    basic.pause(100)\nbasic.forever(on_forever)\ndef on_forever2():\n    global motor\n    if motor == 1:\n        pins.analog_write_pin(AnalogPin.P13, 0)\n        pins.analog_write_pin(AnalogPin.P14, 400)\n        basic.pause(timeHold*1000)\n        motor = 0\n    else:\n        pins.analog_write_pin(AnalogPin.P13, 0)\n        pins.analog_write_pin(AnalogPin.P14, 0)\nbasic.forever(on_forever2)\n\ndef on_forever3():\n    global time, motor, onSetup\n    if timeDelay >= 10:\n        I2C_LCD1602.show_number(timeDelay, 8, 0)\n    else